{
  "name": "stroke-triage-assistant",
  "private": true,
  "version": "2.1.0",
  "type": "module",
  "description": "Professional stroke triage assistant for emergency medical services",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "lint": "eslint src/ --ext .js",
    "lint:fix": "eslint src/ --ext .js --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,json,css,md}\"",
    "quality": "npm run lint && npm run format:check && npm run test:ci",
    "analyze:deps": "madge --circular --extensions js src/",
    "analyze:complexity": "complexity-report --format json src/",
    "analyze:duplication": "jscpd src/",
    "prepare": "husky"
  },
  "devDependencies": {
    "@babel/core": "^7.28.4",
    "@babel/preset-env": "^7.28.3",
    "@tailwindcss/postcss": "^4.1.14",
    "@testing-library/jest-dom": "^6.8.0",
    "@typescript-eslint/parser": "^8.44.1",
    "@vitejs/plugin-react": "^4.2.0",
    "autoprefixer": "^10.4.21",
    "babel-jest": "^30.1.2",
    "complexity-report": "^2.0.0-alpha",
    "dependency-cruiser": "^17.0.1",
    "eslint": "^8.57.1",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-security": "^3.0.1",
    "eslint-plugin-sonarjs": "^3.0.5",
    "husky": "^9.1.7",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^30.1.3",
    "jest-environment-jsdom": "^30.1.2",
    "jscpd": "^4.0.5",
    "lint-staged": "^16.2.1",
    "madge": "^8.0.0",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "tailwindcss": "^4.1.14",
    "vite": "^5.0.0"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.1.14",
    "eslint-plugin-import": "^2.32.0",
    "lucide-react": "^0.544.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "keywords": [
    "stroke",
    "triage",
    "medical",
    "emergency",
    "gfap",
    "ich",
    "lvo"
  ],
  "author": "iGFAP Project",
  "license": "Proprietary"
}
